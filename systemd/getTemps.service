[Unit]
Description=Get Temps
After=multi-user.target

[Service]
Type=simple
User=pi
WorkingDirectory=/home/pi/oneWireTemps
ExecStart=/usr/bin/python3 getTemps.py 

Environment=APP_ENV=production1

Restart=always
RestartSec=10
StartLimitInterval=120
StartLimitBurst=10

[Install]
WantedBy=multi-user.target
